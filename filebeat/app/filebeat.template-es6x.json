{
  "mappings": {
    "_default_": {
      "_meta": {
        "version": "5.6.0"
      },
      "date_detection": false,
      "dynamic_templates": [
        {
          "strings_as_keyword": {
            "mapping": {
              "ignore_above": 1024,
              "type": "keyword"
            },
            "match_mapping_type": "string"
          }
        }
      ],
      "properties": {
        "@timestamp": {
          "type": "date"
        },
        "beat": {
          "properties": {
            "hostname": {
              "ignore_above": 1024,
              "type": "keyword"
            },
            "name": {
              "ignore_above": 1024,
              "type": "keyword"
            },
            "version": {
              "ignore_above": 1024,
              "type": "keyword"
            }
          }
        },
        "offset": {
          "type": "long"
        },
        "source": {
          "ignore_above": 1024,
          "type": "keyword"
        },
        "currency": {"type" : "text"},
        "buying_rate": {"type" : "double"},
        "cash_buying_rate": {"type" : "double"},
        "selling_rate": {"type" : "double"},
        "cach_selling_rate": {"type" : "double"},
        "middle_rate": {"type" : "double"},
        "pub_time": {"type" : "date", "format" : "yyyy-MM-dd HH:mm:ss"}
      }
    }
  },

  "settings": {
    "index.mapping.total_fields.limit": 10000,
    "index.refresh_interval": "5s"
  },
  "template": "filebeat-*"
}